%% empty double page !!! Requires twoside in documentclass to work !!!
\newcommand{\clearemptydoublepage}{
  \phantom{empty}
  \newpage
  \thispagestyle{empty}
  \cleardoublepage
  \thispagestyle{fancy}
}


